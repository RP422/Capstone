<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/EntryDate"
        android:text="Entry Date"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="17sp"
        android:textAlignment="center"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_margin="15sp"/>
    <TextView
        android:id="@+id/AircraftModel"
        android:text="Aircraft Model"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="17sp"
        android:textAlignment="center"
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/EntryDate"
        android:layout_margin="15sp"/>
    <TextView
        android:id="@+id/AircraftID"
        android:text="Aircraft ID"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="17sp"
        android:textAlignment="center"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/AircraftModel"
        android:layout_margin="15sp"/>
    <TextView
        android:id="@+id/Departure"
        android:text="Departure"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="17sp"
        android:textAlignment="center"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/AircraftID"
        android:layout_margin="15sp"/>
    <TextView
        android:id="@+id/Arrival"
        android:text="Arrival"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="17sp"
        android:textAlignment="center"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/Departure"
        android:layout_margin="15sp"/>
    <TextView
        android:id="@+id/NumInstApr"
        android:text="# Instrument\nApproach"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="17sp"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/Arrival"
        android:layout_margin="15sp"/>
    <TextView
        android:id="@+id/RemarksAndEndorsments"
        android:text="Remarks and\nEndorsements"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="17sp"
        android:textAlignment="center"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/NumInstApr"
        android:layout_margin="15sp"/>
    <TextView
        android:id="@+id/DayLandings"
        android:text="Day Landings"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="17sp"
        android:textAlignment="center"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/RemarksAndEndorsments"
        android:layout_margin="15sp"/>
    <TextView
        android:id="@+id/NightLandings"
        android:text="Night Landings"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="17sp"
        android:textAlignment="center"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/DayLandings"
        android:layout_margin="15sp"/>
    <TextView
        android:id="@+id/AircraftClass"
        android:text="Aircraft Class"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="17sp"
        android:textAlignment="center"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/NightLandings"
        android:layout_margin="15sp"/>
    <TextView
        android:id="@+id/AircraftCategory"
        android:text="Aircraft Category"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="17sp"
        android:textAlignment="center"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/AircraftClass"
        android:layout_margin="15sp"/>
    <TextView
        android:id="@+id/FlightTime"
        android:text="Total Time"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="17sp"
        android:textAlignment="center"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/AircraftCategory"
        android:layout_margin="15sp"/>
    <TextView
        android:id="@+id/NightTime"
        android:text="Night"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="17sp"
        android:textAlignment="center"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/FlightTime"
        android:layout_margin="15sp"/>
    <TextView
        android:id="@+id/ActInstTime"
        android:text="Act Inst"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="17sp"
        android:textAlignment="center"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/NightTime"
        android:layout_margin="15sp"/>
    <TextView
        android:id="@+id/SimInstTime"
        android:text="Sim Inst"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="17sp"
        android:textAlignment="center"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/ActInstTime"
        android:layout_margin="15sp"/>
    <TextView
        android:id="@+id/FlgtSimTime"
        android:text="Flight Sim"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="17sp"
        android:textAlignment="center"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/SimInstTime"
        android:layout_margin="15sp"/>
    <TextView
        android:id="@+id/CrossCountryTime"
        android:text="Cross Country"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="17sp"
        android:textAlignment="center"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/FlgtSimTime"
        android:layout_margin="15sp"/>
    <TextView
        android:id="@+id/FlgtInstructorTime"
        android:text="As Flight\nInstructor"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="17sp"
        android:textAlignment="center"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/CrossCountryTime"
        android:layout_margin="15sp"/>
    <TextView
        android:id="@+id/DualReceivedTime"
        android:text="Dual\nReceived"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="17sp"
        android:textAlignment="center"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/FlgtInstructorTime"
        android:layout_margin="15sp"/>
    <TextView
        android:id="@+id/PicTime"
        android:text="Pilot-In-\nCommand"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="17sp"
        android:textAlignment="center"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/DualReceivedTime"
        android:layout_margin="15sp"
        app:layout_constraintVertical_bias="0.49" />


</android.support.constraint.ConstraintLayout>